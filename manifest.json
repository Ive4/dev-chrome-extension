{
  "manifest_version": 2,
  "name": "Cloudinsight Dev",
  "version": "0.0.1",
  "description": "Developer Plugin for Cloudinsight",
  "icons": {
    "128": "icon_128.png"
  },
  "background": {
    "scripts": [
      "lib/background.js"
    ]
  },
  "permissions": [
    "*://*.oneapm.com/*",
    "https://h0r0rop9h6vu9k8oxge5.wilddogio.com/*",
    "alarms",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Cloudinsight Dev",
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },
  "author": "wyvernnot",
  "homepage_url": "https://github.com/cloudinsight/mini-chrome-extension"
}
